{
  "name": "rainbowkit-vue",
  "version": "0.1.0",
  "description": "A Vue 3 port of RainbowKit (WIP)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "styles.css"
  ],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/index.css",
    "./wallets": "./dist/wallets/walletConnectors/index.js"
  },
  "sideEffects": [
    "src/css/reset.css.ts"
  ],
  "engines": {
    "node": ">=12.4"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.vue src"
  },
  "keywords": [
    "vue",
    "ethereum",
    "wallet",
    "blockchain"
  ],
  "author": "Rainbow",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "2.1.9",
    "@vue/test-utils": "^2.4.6",
    "i": "^0.3.7",
    "npm": "^11.4.2",
    "vite": "^5.4.19",
    "vitest": "2.1.9",
    "vue": "^3.5.16",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.80.7",
    "@vanilla-extract/css": "1.17.3",
    "@vanilla-extract/css-utils": "0.1.5",
    "@vanilla-extract/dynamic": "2.1.4",
    "@wagmi/core": "^2.17.3",
    "@wagmi/vue": "^0.1.22",
    "viem": "2.29.2"
  }
}
